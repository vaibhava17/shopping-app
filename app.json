{
  "expo": {
    "name": "Your Shopping List App",
    "slug": "your-shopping-list-app",
    "scheme": "your-shopping-list-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.noobiedev.shoppinglist"
    },
    "android": {
      "package": "com.noobiedev.shoppinglist"
    }
  }
}
